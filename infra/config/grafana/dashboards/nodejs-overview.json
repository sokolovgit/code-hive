{
  "dashboard": {
    "title": "Node.js Service Overview",
    "tags": ["nodejs", "microservices"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_total[5m])) by (service_name)",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" }
          }
        ]
      },
      {
        "id": 2,
        "title": "Request Duration",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_request_duration_seconds_bucket[5m])) by (le, service_name))",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" }
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_total{status_code=~\"5..\"}[5m])) by (service_name)",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" }
          }
        ]
      },
      {
        "id": 4,
        "title": "Active Traces",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "sum(tempo_spanmetrics_calls_total) by (service_name)",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" }
          }
        ]
      }
    ]
  }
}
